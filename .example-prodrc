#!/bin/bash

export SHARE_SPACE_ENV="prod"

# A publicly accessible host that serves the room connection 
export REACT_APP_DEFAULT_ROOM_SERVER="share-space-prod" 

# A privately accessible URL for VNC connections.
# To reference machines by hostname, add a hostname: IP mapping to /etc/hosts
export REACT_APP_VNC_SERVER="http://share-space-prod:6080/vnc.html?host=share-space-prod&port=6080"
